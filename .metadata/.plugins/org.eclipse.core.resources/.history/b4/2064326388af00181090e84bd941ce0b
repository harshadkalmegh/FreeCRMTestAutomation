
public class Switchcase {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i=5;
		
		switch(i)
		{
		
		case 1:
		System.out.println("value is 1");
		break;
		
		case 2:
		System.out.println("value is 2");
		break;
		
		case 3:
		System.out.println("Value is 3");
		break;
		
		case 4:
		System.out.println("Value is 3");
		break;
			
		}

	}

}
